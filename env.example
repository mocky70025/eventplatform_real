# Supabase設定
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# LINE Login設定（開発環境・本番環境共通）
NEXT_PUBLIC_LINE_LOGIN_CHANNEL_ID=your_line_login_channel_id
LINE_LOGIN_CHANNEL_SECRET=your_line_login_channel_secret

# 開発環境用（ngrok使用時）
# NEXT_PUBLIC_LINE_LOGIN_REDIRECT_URI=https://xxxx-xxxx-xxxx.ngrok-free.app/auth/callback
# NEXT_PUBLIC_APP_URL=https://xxxx-xxxx-xxxx.ngrok-free.app

# 本番環境用（Vercel使用時）
# NEXT_PUBLIC_LINE_LOGIN_REDIRECT_URI=https://your-domain.vercel.app/auth/callback
# NEXT_PUBLIC_APP_URL=https://your-domain.vercel.app

# LINE LIFF設定（オプション、現在は使用していません）
# NEXT_PUBLIC_LIFF_ID=your_liff_id

# Vercel設定（オプション）
# VERCEL_URL=your_vercel_url
